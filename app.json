{
  "expo": {
    "name": "Screenhole Next",
    "slug": "screenhole",
    "description": "Post cool grabs on the internet.",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thinko.ScreenholeNext",
      "buildNumber": "1.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos for sharing with others.",
        "NSPhotoLibraryUsageDescription": "This app uses your photo library to pick photos for sharing with others."
      }
    },
    "android": {
      "package": "com.thinko.screenhole"
    },
    "primaryColor": "#7137f9",
    "githubUrl": "https://github.com/screenhole/ios-next",
    "scheme": "screenhole"
  }
}
